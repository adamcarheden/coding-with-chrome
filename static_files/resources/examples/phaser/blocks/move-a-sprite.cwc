  "content": {
    "blockly": "<xml xmlns=\"http://www.w3.org/1999/xhtml\">\n  <block type=\"phaser_game\" x=\"-130\" y=\"-1150\">\n    <field name=\"name\">Move a sprite</field>\n    <field name=\"width\">400</field>\n    <field name=\"height\">600</field>\n    <next>\n      <block type=\"phaser_game_state\">\n        <field name=\"name\">main</field>\n        <field name=\"autostart\">true</field>\n        <statement name=\"state\">\n          <block type=\"phaser_preload\">\n            <statement name=\"CODE\">\n              <block type=\"phaser_load_image\">\n                <field name=\"name\">ship_image</field>\n                <value name=\"image\">\n                  <block type=\"general_file_library_image\">\n                    <field name=\"filename\">\"space-ship.png\"</field>\n                    <field name=\"preview\">data:image/png;base64,</field>\n                  </block>\n                </value>\n              </block>\n            </statement>\n            <next>\n              <block type=\"phaser_create\">\n                <statement name=\"CODE\">\n                  <block type=\"phaser_stage_background_color\">\n                    <field name=\"color\">#ccffff</field>\n                    <next>\n                      <block type=\"phaser_input_keyboard_cursor_keys_add\">\n                        <value name=\"variable\">\n                          <block type=\"variables_get\">\n                            <field name=\"VAR\">cursor_keys</field>\n                          </block>\n                        </value>\n                        <next>\n                          <block type=\"phaser_pyhsics_arcade_sprite_add\">\n                            <field name=\"sprite\">ship_image</field>\n                            <value name=\"variable\">\n                              <block type=\"variables_get\">\n                                <field name=\"VAR\">ship_sprite</field>\n                              </block>\n                            </va